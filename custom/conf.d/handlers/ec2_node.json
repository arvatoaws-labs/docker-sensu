{
  "handlers": {
    "ec2_node": {
      "type": "pipe",
      "filters": [
        "keepalives",
        "ec2",
        "ec2_instance",
        "every_3_occurrences",
        "max_25_occurrences"
      ],
      "severities": [
        "warning",
        "critical"
      ],
      "command": "/etc/sensu/handlers/ruby/ec2_node.rb",
      "timeout": 120,
      "handle_silenced" : true
    }
  }
}
